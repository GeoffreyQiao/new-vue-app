{
  "compilerOptions": {
    "checkJs": true
  },
  "include": [
    "build/**/*",
    "config/**/*",
    "src/**/*"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/*",
    "**/dist/*"
  ],
  "typeAcquisition": {
    "enable": true,
    "include": [
      "node",
      "vue",
      "vue-router",
      "vuex"
    ]
  }
}
